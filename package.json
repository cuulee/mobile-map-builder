{
  "name": "mobile-map-builder",
  "version": "0.1.0",
  "description": "An application that helps you build map data (MBTiles & GeoPackages) from Tile Servers.",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run typings",
    "typings": "./node_modules/typings/dist/bin.js install",
    "test": "npm run build && npm run ava && npm run lint",
    "docker-test": "docker build -t mmb . && docker run --rm -it mmb npm run test",
    "build": "./node_modules/typescript/bin/tsc",
    "ava": "./node_modules/nyc/bin/nyc.js ./node_modules/ava/cli.js --verbose",
    "start": "./node_modules/ts-node/dist/bin.js app/cluster.ts",
    "lint": "./node_modules/tslint/bin/tslint app/**.ts tests/**.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DenisCarriere/mobile-map-builder.git"
  },
  "author": "Denis Carriere",
  "contributors": [
    "Denis Carriere <carriere.denis@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/DenisCarriere/mobile-map-builder/issues"
  },
  "homepage": "https://github.com/DenisCarriere/mobile-map-builder#readme",
  "dependencies": {
    "body-parser": "latest",
    "commander": "latest",
    "concaveman": "^1.1.0",
    "debug": "latest",
    "express": "latest",
    "filesize": "latest",
    "geojson": "^0.3.0",
    "geojson2osm": "0.0.5",
    "js-base64": "latest",
    "js-yaml": "latest",
    "lodash": "latest",
    "multer": "latest",
    "node-uuid": "latest",
    "progress": "latest",
    "request": "latest",
    "request-promise": "latest",
    "sequelize": "latest",
    "sqlite3": "latest",
    "tslint": "latest",
    "turf": "^3.0.14",
    "turf-line-to-polygon": "^0.2.1",
    "validator": "^5.6.0"
  },
  "devDependencies": {
    "ava": "latest",
    "coveralls": "latest",
    "del": "latest",
    "nyc": "latest",
    "ts-node": "latest",
    "tslint": "latest",
    "typescript": "latest",
    "typings": "latest"
  },
  "ava": {
    "files": [
      "dist/tests/*.tests.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true
  }
}
