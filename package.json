{
  "name": "data-generator",
  "version": "1.0.0",
  "description": "An application that helps you build data bundles (MBTiles & GeoPackages) from TileServers.",
  "main": "index.js",
  "scripts": {
    "postinstall": "./node_modules/typings/dist/bin.js install",
    "test": "npm run build && npm run ava",
    "build": "./node_modules/typescript/bin/tsc",
    "ava": "./node_modules/nyc/bin/nyc.js ./node_modules/ava/cli.js --verbose",
    "start": "./node_modules/ts-node/dist/bin.js app/cluster.ts",
    "autostart": "nohup ./node_modules/ts-node/dist/bin.js app/cluster.ts </dev/null &"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dlcspm/data-generator.git"
  },
  "author": "DLCSPM",
  "contributors": [
    "Denis Carriere <denis.carriere@lsec.dnd.ca>"
  ],
  "bugs": {
    "url": "https://github.com/dlcspm/data-generator/issues"
  },
  "homepage": "https://github.com/dlcspm/data-generator#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "lodash": "^4.13.1",
    "mongodb": "^2.2.4",
    "mongoose": "^4.5.5",
    "multer": "^1.1.0",
    "node-uuid": "^1.4.7",
    "request": "^2.74.0",
    "request-promise": "^4.1.1",
    "sequelize": "^3.23.6",
    "sqlite3": "^3.1.4",
    "turf": "^3.0.14"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "coveralls": "^2.11.11",
    "eslint": "^3.0.1",
    "nyc": "^7.0.0",
    "ts-node": "^1.2.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  },
  "ava": {
    "files": [
      "dist/test/*.test.js"
    ],
    "babel": "inherit",
    "concurrency": 5,
    "failFast": true,
    "tap": true
  }
}
