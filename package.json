{
  "name": "data-generator",
  "version": "1.0.0",
  "description": "An application that helps you build data bundles (MBTiles & GeoPackages) from TileServers.",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run ava",
    "build": "./node_modules/.bin/babel app --out-dir=dist --source-maps",
    "ava": "./node_modules/.bin/nyc ./node_modules/.bin/ava --verbose",
    "start": "./node_modules/.bin/nodemon ./node_modules/.bin/babel-node app/cluster.js",
    "autostart": "nohup ./node_modules/.bin/nodemon ./node_modules/.bin/babel-node app/cluster.js </dev/null &",
    "lint": "./node_modules/.bin/eslint app && ./node_modules/.bin/eslint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dlcspm/data-generator.git"
  },
  "author": "DLCSPM",
  "contributors": [
    "Denis Carriere <denis.carriere@lsec.dnd.ca>"
  ],
  "license": "CC0",
  "bugs": {
    "url": "https://github.com/dlcspm/data-generator/issues"
  },
  "homepage": "https://github.com/dlcspm/data-generator#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "lodash": "^4.13.1",
    "mongoose": "^4.5.4",
    "multer": "^1.1.0",
    "node-uuid": "^1.4.7",
    "sqlite3": "^3.1.4",
    "turf": "^3.0.14"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.11.11",
    "eslint": "^3.0.1",
    "ignore-styles": "^4.0.0",
    "nodemon": "^1.9.2",
    "nyc": "^7.0.0",
    "ts-node": "^0.9.3"
  },
  "ava": {
    "files": [
      "test/test_*.js"
    ],
    "babel": "inherit",
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register",
      "ignore-styles"
    ]
  }
}
