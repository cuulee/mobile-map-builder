{
  "name": "data-generator",
  "version": "1.0.0",
  "description": "An application that helps you build data bundles (MBTiles & GeoPackages) from TileServers.",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run typings && npm run build",
    "typings": "./node_modules/typings/dist/bin.js install",
    "test": "npm run build && npm run ava && npm run lint",
    "docker-test": "docker build -t data-generator . && docker run --rm -it data-generator npm run test",
    "build": "./node_modules/typescript/bin/tsc",
    "ava": "./node_modules/nyc/bin/nyc.js ./node_modules/ava/cli.js --verbose",
    "start": "./node_modules/ts-node/dist/bin.js app/cluster.ts",
    "lint": "./node_modules/tslint/bin/tslint app/**.ts test/**.ts",
    "autostart": "nohup ./node_modules/ts-node/dist/bin.js app/cluster.ts </dev/null &"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dlcspm/data-generator.git"
  },
  "author": "DLCSPM",
  "contributors": [
    "Denis Carriere <denis.carriere@lsec.dnd.ca>"
  ],
  "bugs": {
    "url": "https://github.com/dlcspm/data-generator/issues"
  },
  "homepage": "https://github.com/dlcspm/data-generator#readme",
  "dependencies": {
    "body-parser": "latest",
    "commander": "latest",
    "debug": "latest",
    "express": "latest",
    "filesize": "latest",
    "geojson2osm": "0.0.5",
    "js-base64": "latest",
    "js-yaml": "latest",
    "lodash": "latest",
    "multer": "latest",
    "node-uuid": "latest",
    "progress": "latest",
    "request": "latest",
    "request-promise": "latest",
    "sequelize": "latest",
    "sqlite3": "latest",
    "tslint": "latest",
    "turf": "latest"
  },
  "devDependencies": {
    "ava": "latest",
    "coveralls": "latest",
    "del": "latest",
    "nyc": "latest",
    "ts-node": "latest",
    "tslint": "latest",
    "typescript": "latest",
    "typings": "latest"
  },
  "ava": {
    "files": [
      "dist/test/*.test.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true
  }
}
